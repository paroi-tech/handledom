{
  "name": "@handledom/in-template-string-loader",
  "version": "0.0.1",
  "license": "CC0-1.0",
  "description": "A webpack loader that compiles Handledom templates in template strings at build time.",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "clear": "rimraf 'dist/*'",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "build": "npm run clear && npm run tsc",
    "lint": "tslint -p tsconfig.json -t verbose",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "handledom": "^0.0.2",
    "loader-utils": "^1.2.3"
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/loader-utils": "^1.1.3",
    "@types/node": "^12.12.7",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomko-team/@handledom/in-template-string-loader.git"
  },
  "keywords": [
    "handledom",
    "template",
    "string",
    "webpack"
  ]
}
